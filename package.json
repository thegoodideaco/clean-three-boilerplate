    "@tweakpane/plugin-essentials": "^0.2.1",
    "@vueuse/core": "^10.7.1",
    "d3": "^7.8.5",
    "glsl-noise": "^0.0.0",
    "gsap": "^3.12.4",
    "lodash": "^4.17.21",
    "maath": "^0.10.7",
    "pinia": "^2.1.7",
    "pixi.js": "^7.3.3",
